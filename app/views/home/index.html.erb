<header>
  <h1><%= @brand %></h1>
</header>
<table>
  <thead>
    <tr>
      <th>Last 5 Tweets</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tweet5.each do |tweet5| %>
      <tr>
        <td><%= tweet5.message %></td>
        <td><%= time_ago_in_words(tweet5.created_at)%></td>
      </tr>
    <% end %>
  </tbody>
</table>
